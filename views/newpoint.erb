<form class="rebus-form">
  <div class="page-header">
    <h1>
      Rebus 
      <i class="icon-left-bold"></i> <%= pos %> <i class="icon-right-bold"></i>
    </h1>
  </div>
  <textarea class="span3" placeholder="Write a rebus here that gives the participant an idea of where to go."></textarea>
  <div class="form-inline pointpos">
    <label>Latitude</label>
    <input type="text" class="span1 disabled" disabled value="<%= location["lat"] %>">
    <label>Longitude</label>
    <input type="text" class="span1 disabled" disabled value="<%= location["lng"] %>">
  </div>
  <input type="range"  min="0" max="100" />
</form>
